<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>web_ve_xe</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>

	<servlet>
        <servlet-name>Homepage</servlet-name>
        <servlet-class>control.HomeControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Homepage</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>


	<servlet>
        <servlet-name>FindTicket</servlet-name>
        <servlet-class>control.FindTicket</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindTicket</servlet-name>
        <url-pattern>/findticket</url-pattern>
    </servlet-mapping>
    
	<servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>control.LogoutControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    
</web-app>